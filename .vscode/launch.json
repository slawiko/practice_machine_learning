{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [

    {
      "name": "applicator",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/hw4/dummy_applicator.py",
      "console": "integratedTerminal",
      "args": [
        "./dist/model_with_step.csv",
        "./hw4/train/cars/0a77ee5f4a74d7e546a1856a5ebe62fc.bmp"
      ]
    },
    {
      "name": "learner",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/hw4/dummy_learner.py",
      "console": "integratedTerminal",
      "args": [
        "./hw4/train", 
        "./dist/model_with_step.csv"
      ]
    },
    {
      "name": "checker",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/hw4/checker.py",
      "console": "integratedTerminal",
      "args": [
        "./hw4/dummy_applicator.py",
        "./dist/model_with_step.csv",
        "./hw4/train"
      ]
    },
    {
      "name": "Python: Current File (Integrated Terminal)",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Attach",
      "type": "python",
      "request": "attach",
      "port": 5678,
      "host": "localhost"
    },
    {
      "name": "Python: Django",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "console": "integratedTerminal",
      "args": [
        "runserver",
        "--noreload",
        "--nothreading"
      ],
      "django": true
    },
    {
      "name": "Python: Flask",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "app.py"
      },
      "args": [
        "run",
        "--no-debugger",
        "--no-reload"
      ],
      "jinja": true
    },
    {
      "name": "Python: Current File (External Terminal)",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "externalTerminal"
    }
  ]
}